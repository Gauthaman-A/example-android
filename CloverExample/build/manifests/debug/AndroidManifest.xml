<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.clover.cloverexample">

    <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="16"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.clover.cloverexample.MainActivity">
            <intent-filter>
                <action android:name="clover.intent.action.PAY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_list_orders" android:name="com.clover.cloverexample.ListOrders">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="RewardsActivity" android:name="com.clover.cloverexample.RewardsActivity">
        </activity>
    </application>

</manifest>
